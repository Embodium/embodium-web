{
  "extends": "tsconfig/base.json",
  "compilerOptions": {
    "allowJs": true,
    "target": "ES2017",
    "module": "ES2020",
    "lib": ["es2020", "DOM"],
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik",
    "strict": true,
    "emitDeclarationOnly": true,
    "declaration": true,
    "declarationDir": "@types",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "incremental": true,
    "isolatedModules": true,
    "types": ["vite/client", "@builder.io/qwik", "@types/carbon__feature-flags"],
  },
    "include": ["src/**/*"]
  }
